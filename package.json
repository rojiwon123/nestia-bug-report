{
    "name": "nestia-bug-report",
    "version": "0.0.0",
    "main": "build/main",
    "scripts": {
        "--------------------runner--------------------": "",
        "start": "npm run build && NODE_ENV=development node build/main",
        "bug": "npm run build:prisma && npm run build:swagger",
        "--------------------build--------------------": "",
        "build": "rimraf ./build && tsc -p ./tsconfig.build.json",
        "build:prisma": "rimraf ./prisma/client && prisma generate",
        "build:swagger": "rimraf ./docs/swagger.json && NODE_ENV=test nestia swagger",
        "build:sdk": "rimraf ./sdk && NODE_ENV=test nestia sdk && rimraf ./packages/sdk/lib && tsc -p ./tsconfig.sdk.json && rimraf ./sdk",
        "build:nestia": "npm run build:sdk && npm run build:swagger",
        "check": "ts-patch check",
        "postinstall": "husky",
        "prepare": "ts-patch install && typia patch",
        "prettier:fix": "prettier . -w"
    },
    "author": "rojiwon123",
    "license": "UNLICENSED",
    "keywords": [
        "nestjs",
        "nestia",
        "serverless"
    ],
    "dependencies": {
        "@codegenie/serverless-express": "^4.14.1",
        "@fxts/core": "^0.29.0",
        "@nestia/core": "^3.2.0",
        "@nestia/fetcher": "^3.2.0",
        "@nestjs/common": "^10.3.8",
        "@nestjs/core": "^10.3.8",
        "@nestjs/platform-express": "^10.3.8",
        "aws-lambda": "^1.0.7",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "helmet": "^7.1.0",
        "rxjs": "^7.8.1",
        "typia": "^6.0.3",
        "winston": "^3.13.0"
    },
    "devDependencies": {
        "@nestia/e2e": "^0.4.3",
        "@nestia/sdk": "^3.2.0",
        "@prisma/client": "^5.14.0",
        "@project/api": "file:packages/api",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/aws-lambda": "^8.10.138",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.21",
        "@types/node": "^20.12.12",
        "@typescript-eslint/eslint-plugin": "^7.9.0",
        "@typescript-eslint/parser": "^7.9.0",
        "eslint-plugin-deprecation": "^2.0.0",
        "husky": "^9.0.11",
        "nestia": "^5.3.0",
        "prettier": "^3.2.5",
        "prisma": "^5.14.0",
        "prisma-markdown": "^1.0.9",
        "rimraf": "^5.0.7",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "ts-patch": "^3.1.2",
        "typescript": "^5.4.5",
        "typescript-transform-paths": "^3.4.7"
    }
}
